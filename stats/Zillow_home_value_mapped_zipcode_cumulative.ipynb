   "source": [
    "def get_remaining_zipcodes_zipcodes(remaining_zipcodes, boro):\n",
    "    return remaining_zipcodes[remaining_zipcodes[\"borough\"] == boro]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "id": "251bf399",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Non intersecting zipcodes\n",
    "def create_remaining_zipcode_csvs(remaining_zipcodes, display=False):\n",
    "    boros = [\"BRONX\", \"BROOKLYN\", \"QUEENS\", \"MANHATTAN\", \"STATEN_ISLAND\"]\n",
    "    for boro in boros:\n",
    "        data = get_remaining_zipcodes_zipcodes(remaining_zipcodes, boro)\n",
    "        create_csv(data, \"remaining_zipcodes\", \"remaining_zipcode_home_value\", boro, display)"
