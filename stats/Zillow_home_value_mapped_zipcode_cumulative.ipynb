    "### Test for normality"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "03e785e9",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Generate Shapiro wilk test for \n",
    "def generate_shapiro_test(all_boro_home_value, boro):\n",
    "    data = all_boro_home_value[all_boro_home_value[\"borough\"] == boro]\n",
    "    shapiro_wilk_data = stats.shapiro(data['home_value'])\n",
    "    print(boro)\n",
    "    print(\"W={}, P={}\".format(shapiro_wilk_data[0], shapiro_wilk_data[1]))"
   ]
  },
