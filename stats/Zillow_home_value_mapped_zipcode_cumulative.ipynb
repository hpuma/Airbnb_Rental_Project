   "source": [
    "### Map home value to top airbnb zipcodes for each borough in NYC"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "1ef4a750",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Helper function that creates csv file from data frame\n",
    "def create_csv(data, folder_name, fname_suffix, boro, display_=False):\n",
    "    outdir = \"./zipcode_data/{}\".format(folder_name)\n",
    "    if not os.path.exists(outdir):\n",
    "        os.mkdir(outdir)\n",
    "    if display_:\n",
    "        print(\"Borough={}\".format(boro))\n",
    "        display(data)\n",
