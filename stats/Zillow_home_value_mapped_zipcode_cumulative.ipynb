   "source": [
    "Helper function to narrow down each data frame to borough and map home prices for that year"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "dba2deec",
   "metadata": {},
   "outputs": [],
   "source": [
    "def merge_zipcode_home_value(zipcodes, zillow_home_value, boro):\n",
    "    # Narrow down by borough\n",
    "    merge_cols = ['zipcode','year']   \n",
    "    boro_zipcodes = zipcodes[zipcodes[\"borough\"] == boro]\n",
    "    # And remove \"borough\" column to prevent duplicates on merge\n",
    "    boro_home_value = zillow_home_value[zillow_home_value[\"borough\"]== boro].drop([\"borough\"], axis=1)\n",
    "    \n",
    "    merged_df = pd.merge(boro_zipcodes, boro_home_value,  how='left', left_on=merge_cols, right_on=merge_cols)\n",
    "    return merged_df"
   ]
  },
